        <TextBox x:Name="txtb_guthaben" Text="aktuelles Guthaben" TextWrapping="Wrap"
 Grid.Row="6"  Grid.Column="20" Grid.ColumnSpan="4" VerticalAlignment="Center" HorizontalAlignment="Left"
 FontSize="15" Foreground="White" Background="Transparent" BorderBrush="Transparent"/>

