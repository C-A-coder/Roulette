        <Button x:Name="btn_Coint100" Content="100" Grid.Column="7" Grid.ColumnSpan="2" Grid.Row="7" FontSize="20" Height="80" Width="80" Click="btn_Coint100_Click" BorderBrush="Transparent">
            <Button.Background>
                <ImageBrush ImageSource="./Bilder_Roulette/pink.png"/>
