        <Button x:Name="btn_Coint1000" Content="1000" Grid.Column="15" Grid.ColumnSpan="2" Grid.Row="7" FontSize="20" Height="80" Width="80" Click="btn_Coint1000_Click" BorderBrush="Transparent">
