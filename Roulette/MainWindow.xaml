        </Button>

        <Button x:Name="btn_Coint500" Content="500" Grid.Column="12" Grid.ColumnSpan="2" Grid.Row="7" FontSize="20" Height="80" Width="80" Click="btn_Coint500_Click" BorderBrush="Transparent">
            <Button.Background>
                <ImageBrush ImageSource="./Bilder_Roulette/dunkelblau.png"/>
